<table class="table">
  <thead class="thead-dark">
    <tr>
      <th  scope="col">Title</th>
      <%# <th  scope="col"> Image  </th> %>
      <%# <th  scope="col">Description</th> %>
      <th  scope="col">Price</th>
      <th scope="col">items in cart</th>
      <th  scope="col">Total</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @arr.each do |product| %>
      <tr>
        <td scope="row"><%= product[:product_name] %></td> 
        <td scope="row"><%= product[:product_price] %> </td>
        <td scope="row"><%= product[:quantity]%></td>
        <td scope="row"><%= product[:product_total]%></td>
      </tr>      
    <% end %>
  </tbody>
</table>
<div>
  <%= link_to "CheckOut" ,  order_index_path %>
</div>
