<h1>Search Result </h1>
<p> <%= link_to 'Back' , new_search_path %> </p>
    <% if @search.search_products.empty? %>
        <p> No Products Found </p>
    <% else %>
        <% @search.search_products.each do |c| %>
            <br>
            <div class= "div">
                <h1><%= c.title %> </h1>
                <p> Price: $<%= c.price%> </p>
                <p> Category: <%= c.Category%> </p>
                <p> Brand: <%= c.brand%> </p>
                <p> Store: <%= c.store%> </p>
                <p> Description: <%= c.description%> </p>
            </div>
        <% end %>
    <% end %>