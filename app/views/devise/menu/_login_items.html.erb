<% if user_signed_in? %>
  <li>
  <%= link_to('Logout', destroy_user_session_path, method: :delete) %>        
  </li>
<% elsif not user_signed_in? %>
  <li>
  <%= link_to('Login as Admin', new_user_session_path)  %>  
  </li>
<% end %>

<% if seller_signed_in? %>
  <li>
    <%= link_to('Logout', destroy_seller_session_path, method: :delete)  %>
  </li>
  <% else %>
    <li>
  <%= link_to('Login as Seller', new_seller_session_path)  %>        
    </li>
<% end %>